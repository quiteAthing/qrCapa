						<option disabled="true">Alphanumeric</option>
						<option disabled="true" >numeric</option>
					</select>
				
				</td>
				<td>
					<select onchange="updateCapacity()" id="ecc">
						<option selected="selected">L</option>
						<option>M</option>
						<option>H</option>
						<option>Q</option>
					</select>
				</td>
				<td><span id="capacity"></span><span>位元</span></td>				
				<td><span id="volA"></span><span>%</span></td>
				<td><span id="volB"></span><span>%</span></td>
				
			</tr>
			
			
		
		</table>
